4)
d = {1: 10, 2: 20, 3: 30, 4: 40, 5: 50, 6: 60}

def check(n):
  if n in d:
    print('yes')
  else:
    print('no')
  
check(10)

print(len(d))

 def check(n):
   for i in range(1,len(d)+1):
     if d[i]==n:
       print(i)
 check(60)

