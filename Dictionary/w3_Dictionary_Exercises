6)

d={}

def new_d(n):
  for i in range(1,n+1):
    d[i]=i**2
  print(d)
new_d(5)

7)

d={}

def new_d(n):
  for i in range(1,n+1):
    d[i]=i**2
  print(d)
new_d(15)

8)

d1 = {'a': 100, 'b': 200}
d2 = {'x': 300, 'y': 200}
d={}
d3={}
for i in (d1,d2):
  d.update(i)
print(d)

d3=d1.copy()
d3.update(d2)
print(d3)

